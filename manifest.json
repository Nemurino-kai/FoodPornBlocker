{
    "manifest_version": 2,
    "name":"FoodPornBlocker",
    "description": "Twitter上の飯テロ画像をブロックします",
    "version": "1.0.0",
    "content_scripts": [{
        "matches": [ "<all_urls>" ],
        "js": ["p5.min.js","p5.dom.min.js","p5.sound.min.js","ml5.min.js","jquery.min.js", "image_replace.js" ]
    }],
    "permissions":[
        "*://*.twitter.com/*",
        "https://pbs.twimg.com/*"
    ],
    "web_accessible_resources":[
        "mobilenet_v1_025_224/*",
        "test.jpg"
    ]
}